;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoKA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA","file":"AddDevice.vue","sourceRoot":"src/components","sourcesContent":["<template>  \n<div class=\"add-device\">\n        <v-menu top :close-on-content-click=\"closeOnContentClick\" style=\"border-raduis:100%;max-width:40px\">\n            <template v-slot:activator=\"{ on }\">\n                <div dark v-on=\"on\" > \n                    <v-btn class=\"mx-2\" fab dark color=\"rgb(220, 220, 220)\" style=\"margin:10px;width:30px; height:30px\">\n                        <v-icon dark>mdi-plus</v-icon>\n                    </v-btn><span style=\"color:rgb(200, 200, 200);font-size:10px;margin-top:5px\"><strong>ADD</strong></span>\n                </div>\n            </template>\n\n            <v-list>\n                <v-list-item v-for=\"(title, index) in devicesNames\" :key=\"index\" @click=\"selectDevice(title.text)\" >\n                <v-list-item-title>{{ title.text }}</v-list-item-title>\n                </v-list-item>\n            </v-list>\n        </v-menu>\n        \n    <v-dialog\n      v-model=\"dialog\"\n      max-width=\"290\"\n    >\n      <v-card>\n        <v-card-title class=\"headline\">Confirm</v-card-title>\n\n        <v-card-text>\n            <span >Would like to add the <b>{{selectedDevice}}?</b></span>\n        </v-card-text>\n\n        <v-card-actions>\n          <div class=\"flex-grow-1\"></div>\n\n          <v-btn\n            color=\"gray\"\n            text\n            @click=\"dialog = false\" >\n            ABANDON\n          </v-btn>\n        <v-btn\n            :disabled=\"loadingDialog\"\n            :loading=\"loadingDialog\"\n            color=\"gray\"\n            @click=\"onAddClicked\"> \n            ADD\n        </v-btn>\n            <v-dialog\n            v-model=\"loadingDialog\"\n            hide-overlay\n            persistent\n            width=\"300\"\n            >\n            <v-card color=\"dark\" dark >\n                <v-card-text>\n                Please wait\n                <v-progress-linear\n                    indeterminate\n                    color=\"white\"\n                    class=\"mb-0\"\n                ></v-progress-linear>\n                </v-card-text>\n            </v-card>\n            </v-dialog>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n</div>      \n\n\n</template>\n\n<script>\nimport {createNamespacedHelpers} from 'vuex'\nconst {mapGetters} = createNamespacedHelpers('users')\nexport default {\n    name:'AddDevice',\n    props:['id'],\n\n    data(){\n        return{\n            showList: false,\n            dialog: false,\n            loadingDialog :false,\n            selectedDevice:'',\n            deviceName:'',\n            device_imgUrl:'',\n            roomID:'',\n            deviceStatus: false,\n            devicesNames: [\n                { text: 'Lamp' },\n                { text: 'Coffe maker' },\n                { text: 'Air-conditioner' },\n                { text: 'Dishwasher' },\n                { text: 'SmartTv' },\n                { text: 'Windows blind' },\n            ],\n            closeOnContentClick: true,\n        }\n    },\n        computed:{\n            ...mapGetters(['user'])\n        },\n        \n    methods:{\n        selectDevice(value){\n             this.selectedDevice = value\n             this.dialog = true\n\n            switch(this.selectedDevice){\n                case'Lamp':\n                this.deviceName = 'Lamp'\n                this.device_imgUrl = 'lamp-off'\n                break\n                case'Coffe maker':\n                this.deviceName = 'Coffee_Maker'\n                this.device_imgUrl = 'coffee_maker'\n                break\n                case'Air-conditioner':\n                this.deviceName = 'Air_conditioner'\n                this.device_imgUrl = 'air_conditioner'\n                break\n                case'Dishwasher':\n                this.deviceName = 'Dishwasher'\n                this.device_imgUrl = 'dishwasher'\n                break\n\n                case'SmartTv':\n                this.deviceName = 'SmartTv'\n                this.device_imgUrl = 'smart_tv'\n                break\n\n                case'Windows blind':\n                this.deviceName = 'Window blind'\n                this.device_imgUrl = 'window_blind'\n                break\n            }\n        },\n\n\n        onAddClicked () {          \n            this.loadingDialog = true\n            this.$store.dispatch('users/addDevice',{userId: this.user.id, roomId: this.id, deviceName: this.deviceName, imgUrl: this.device_imgUrl, status: this.deviceStatus})\n            this.$store.dispatch('users/fetchDevices',{userId: this.user.id, roomId: this.id})\n        },\n    \n    \n    },\n\n     watch: {\n      loadingDialog (val) {\n        if (!val) return\n        setTimeout(() => (this.loadingDialog = false), 500)\n        this.dialog = false\n      },\n\n      user(value){\n          if(value !== null || value !== undefined)\n          console.log(this.user)\n      }\n    },\n    \n}\n</script>\n\n<style scoped>\n    .add-device{\n        cursor:pointer;\n        text-align: center;\n        border-radius: 100%;\n        font-size: 12px;\n        padding: 5px;\n    }\n\n    .add-btn{\n        text-align: center;\n        display: flex;\n        justify-content: space-between;\n        margin: 10px; \n    }\n\n    .add-device:hover {\n        box-shadow: 0 10px 19px 0 rgba(0,0,0,0.2);\n        background: grey;\n        }\n\n     .headline{\n         background: rgb(233, 226, 226);\n         padding: 20px;\n         text-align: center;\n         margin-bottom: 10px;\n     }   \n\n</style>"]}]}