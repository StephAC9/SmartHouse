AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA","file":"SignIn.vue","sourceRoot":"src/components","sourcesContent":["<template>\n    <div class=\"welcome-container\">\n            <div class=\"container-child\"></div>\n        \n            <div class=\"container-child\">\n                <v-card class=\"elevation-12\">\n                    <v-toolbar color=\"dark\" dark >\n                        <v-toolbar-title style=\"color:gray\">Login</v-toolbar-title>\n                        <div class=\"flex-grow-1\"></div>\n                    </v-toolbar>\n                    <v-card-text>\n                        <v-form>\n                            <div class=\"login-detail\">\n                                <span style=\"margin-top:24px;margin-right:10px\"><v-icon>{{ icons.mdiAccount }}</v-icon></span>\n                                \n                                <v-text-field\n                                    v-model=\"email\"\n                                    :rules=\"emailRules\"\n                                    color=\"dark\"\n                                    label=\"Email\"\n                                    name=\"email\"\n                                    type=\"email\" >\n                                </v-text-field>\n                            </div>\n                            <div class=\"login-detail\">\n                                <span style=\"margin-top:24px;margin-right:10px\"><v-icon>{{ icons.mdiLock }}</v-icon></span>\n                                <v-text-field\n                                    v-model=\"password\"\n                                    id=\"password\"\n                                    color=\"dark\"\n                                    label=\"Password\"\n                                    name=\"password\"\n                                    type=\"password\" >\n                                </v-text-field>\n                            </div>\n                        </v-form>\n                            <v-card-actions style=\"margin-top:20px\">\n                                <div class=\"flex-grow-1\"></div>\n                                <v-btn @click=\"logMeIn\"  color=\"dark\" style=\"margin-right:10px; font-size:10px; font-style:bold\"><strong>Login</strong></v-btn>\n                            </v-card-actions>\n                    </v-card-text>\n                    <v-btn @click=\"gotoSignup\" text style=\"margin: 10px\"><label><strong>Create new account</strong></label></v-btn><hr> \n                    <v-btn x-large color=\"rgb(250, 230, 197)\" dark style=\"width:100%; font-size:10px\"><label><strong>Recover your password</strong></label></v-btn>                    \n                    </v-card>\n\n                    <v-dialog\n                        v-model=\"show_loginFailed_dialog\"\n                        hide-overlay\n                        persistent\n                        max-width=\"290\"\n                        >\n                        <v-card color=\"red\">\n\n                            <v-card-title class=\"headline\"><b>Oops!!</b></v-card-title>\n                        </v-card>\n\n                        <v-card-text class=\"white\">\n                            <span style=\"color:red\">Incorrect Email or Password!</span>\n                        </v-card-text>\n\n                        <v-card-actions class=\"white\">\n\n                        <div class=\"flex-grow-1\"></div>\n                        <v-btn color=\"gray\" @click=\"show_loginFailed_dialog = false\"> \n                            OK\n                        </v-btn>\n                        </v-card-actions>\n                </v-dialog>\n\n            </div>\n            <div class=\"container-child\">\n                \n            </div> \n    </div>      \n</template>\n\n<script>\n    import {\n    mdiAccount,\n    mdiDelete,\n    mdiLock\n    } from '@mdi/js'\n    import {createNamespacedHelpers} from 'vuex'\n    const {mapGetters} = createNamespacedHelpers('users')\n\nexport default {\n    name :'WelcomePage',\n\n     props: {\n      prepend: String,\n    },\n\n    data(){\n        return{\n            show_loginFailed_dialog: false,\n            email:'',\n            password:'',\n            icons: {\n                mdiAccount,\n                mdiDelete,\n                mdiLock\n            },\n            emailRules: [\n                v => /^(([^<>()\\[\\]\\\\.,;:\\s@\"]+(\\.[^<>()\\[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/.test((v).toLowerCase()) || 'Invalid E-mail address',\n            ],\n        }\n    },\n    computed:{\n        ...mapGetters(['user']),\n        ...mapGetters(['getUser_not_found']),\n        ...mapGetters(['getsignInSuccessAlert'])\n    \n    },\n\n    methods: {\n    logMeIn(){\n        console.log('signing in...')\n        console.log(this.getUser_not_found)\n        this.$store.dispatch('users/signIn',{email: this.email, password: this.password})\n\n         setTimeout(()=>{\n             if(this.getUser_not_found === true){\n                this.email = ''\n                this.password = ''\n                this.show_loginFailed_dialog = true\n                }\n            },2500);\n    },\n     gotoSignup(){\n        this.$router.push('/signup')\n    }\n    },\n     created () {\n      this.$vuetify.theme.dark = false\n    },\n\n    watch:{\n        user(value){\n          if(value !== null || value !== undefined){\n              setTimeout(()=>{\n              this.$router.push({name:'dashboard'})\n            },1000);\n          }\n        },\n    }\n\n}\n</script>\n\n<style scoped>\n\n.welcome-container{\n    align-content: center;\n    margin: 10%;\n    display: flex;\n    justify-content: space-evenly;\n}\n\n.container-child{\n    min-width: 270px;\n    flex-basis: 30%;\n    align-content: center;\n}\n\n.login-detail{\n    text-align: center;\n    display: flex;\n}\n\nlabel{\n    color:rgb(93, 107, 112);\n}\nlabel:hover{\n    color: rgb(78, 240, 240);\n}\n\n\n\n\n\n</style>"]}]}